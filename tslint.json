{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "no-console": false,
        "typedef":[
            true,
            "call-signature",
            "variable-declaration",
            "property-declaration"
        ],
        "no-bitwise": false,
        "no-var-keyword": true
    },
    "rulesDirectory": []
}